{
  "name": "anysql-cordova-sqlite",
  "version": "0.4.2",
  "description": "Cordova SQLite implementation of the AnySQL API",
  "author": "Manuel Vila <mvila@3base.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/object-layer/anysql-cordova-sqlite"
  },
  "scripts": {
    "compile": "babel src --out-dir lib",
    "prepublish": "npm run compile",
    "release": "npm run lint && npm run compile && npm test && (git checkout --quiet master && test -z \"$(git status --porcelain)\" && npm version $npm_config_release_type && git push --follow-tags) || echo \"An error occured (unclean repository?)\"",
    "release-patch": "npm run release --release-type=patch",
    "release-minor": "npm run release --release-type=minor",
    "release-major": "npm run release --release-type=major",
    "lint": "eslint .",
    "test": "echo \"Warning: no test specified\" && exit 0"
  },
  "dependencies": {
    "await-lock": "^1.1.1",
    "sleep-promise": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-plugin-transform-async-to-generator": "^6.4.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^2.0.0-beta.1",
    "eslint-config-next": "^0.1.0"
  }
}
